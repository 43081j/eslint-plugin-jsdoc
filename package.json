{
  "name": "eslint-plugin-jsdoc",
  "description": "JSDoc linting rules for ESLint.",
  "version": "2.3.1",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/eslint-plugin-jsdoc"
  },
  "keywords": [
    "eslint",
    "plugin",
    "jsdoc"
  ],
  "author": {
    "name": "Gajus Kuizinas",
    "email": "gajus@gajus.com",
    "url": "http://gajus.com"
  },
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "dependencies": {
    "comment-parser": "^0.4.0",
    "lodash": "^4.5.1"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./dist --copy-files --source-maps",
    "lint": "eslint ./src ./test",
    "test": "mocha --compilers js:babel-register && npm run lint",
    "add-assertions": "babel-node --presets es2015 ./bin/readme-assertions",
    "generate-readme": "gitdown ./.README/README.md --output-file ./README.md && npm run readme-add-assertions"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.10.2",
    "eslint-config-canonical": "^5.5.0",
    "gitdown": "^2.4.0",
    "globby": "^4.0.0",
    "mocha": "^2.5.3"
  }
}
